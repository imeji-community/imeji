				<xsd.resolution.thumbnail>103</xsd.resolution.thumbnail>
				<xsd.resolution.web>357</xsd.resolution.web>
                
                <!-- Cone -->
				<escidoc.cone.service.url>http://dev-pubman.mpdl.mpg.de:8080/cone/</escidoc.cone.service.url>
			
			</properties>
